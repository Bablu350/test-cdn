{
    "openapi": "3.0.0",
    "info": {
        "title": "APIs For Home Page",
        "version": "1.0.0"
    },
    "paths": {
        "/graphql": {
            "post": {
                "tags": [
                    "Get Clinic Data"
                ],
                "summary": "Get clinic data",
                "description": "Retrieve clinic data using GraphQL",
                "operationId": "83347629b53d70b40a2796768e1062de",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "header",
                        "description": "Provide UUID",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "81c7aec7-9836-4945-a4df-ba6c3d493563"
                    },
                    {
                        "name": "encodekey",
                        "in": "header",
                        "description": "Provide Encode Key",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "9053a884b2c04c33313154d8f92b780f"
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "query": {
                                        "type": "string",
                                        "example": "query { getClinicData { status message data { heading description favicon color_code url } } }"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "status": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "type": "object"
                                        },
                                        "message": {
                                            "type": "string"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
